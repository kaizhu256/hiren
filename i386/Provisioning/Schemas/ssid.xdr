<Schema name="http://www.microsoft.com/provisioning/SSID"
        xmlns="urn:schemas-microsoft-com:xml-data" 
        xmlns:dt="urn:schemas-microsoft-com:datatypes">

    <AttributeType name="Name" dt:type="string" />

    <ElementType name="Priority" content="textOnly" dt:type="ui4" /> <!-- type="xsd:positiveInteger"-->
    <ElementType name="Connection" content="textOnly" dt:type="enumeration" dt:values="IBSS ESS" />
    <ElementType name="Authentication" content="textOnly" dt:type="enumeration" dt:values="Open Shared WPA WPAPSK" />
    <ElementType name="Encryption" content="textOnly" dt:type="enumeration" dt:values="None WEP TKIP CCMP" />
    <ElementType name="KeyIndex" content="textOnly" dt:type="ui4" /> <!-- type="xsd:positiveInteger"-->
    <ElementType name="IEEE802.1X" content="textOnly" dt:type="string" />
    <ElementType name="Non802.1XURL" content="textOnly" dt:type="string" /> <!-- type="xsd:anyURI"-->


    <ElementType name="ssid" content="eltOnly" order="seq" model="closed">
	<element type="Priority" minOccurs="0" maxOccurs="1" />
	<element type="Connection" minOccurs="0" maxOccurs="1" />
	<element type="Authentication" minOccurs="0" maxOccurs="1" />
	<element type="Encryption" minOccurs="0" maxOccurs="1" />
	<element type="KeyIndex" minOccurs="0" maxOccurs="1" />
	<element type="IEEE802.1X" minOccurs="0" maxOccurs="1" />
	<element type="Non802.1XURL" minOccurs="0" maxOccurs="1" />
	<attribute type="Name" required="yes"/>
    </ElementType>

    <ElementType name="SSIDs" content="eltOnly" order="seq" model="closed">
	<element type="ssid" minOccurs="1" maxOccurs="*"/>
    </ElementType>

</Schema>
