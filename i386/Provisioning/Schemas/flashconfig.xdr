<?xml version="1.0" ?>
<Schema 
      name="http://www.microsoft.com/provisioning/WirelessProfile/2004"
      xmlns="urn:schemas-microsoft-com:xml-data"
      xmlns:dt="urn:schemas-microsoft-com:datatypes">

   <ElementType name="wirelessProfile" content="eltOnly">
	<element type="config" 				minOccurs="1" maxOccurs="1" />
	<element type="ssid" 				minOccurs="1" maxOccurs="1" />
	<element type="connectionType" 		minOccurs="1" maxOccurs="1" />
	<element type="channel2Dot4" 		minOccurs="0" maxOccurs="1" />
	<element type="channel5Dot0" 		minOccurs="0" maxOccurs="1" />
	<element type="deviceMode" 			minOccurs="0" maxOccurs="1" />
	<element type="primaryProfile" 		minOccurs="1" maxOccurs="1" />
	<element type="optionalProfile"		minOccurs="0" maxOccurs="*" />
   </ElementType>

   <ElementType name="ssid" 			dt:type="string" content="textOnly" dt:minLength="1" dt:maxLength="32"/>
   <ElementType name="connectionType" 	dt:type="enumeration" dt:values="IBSS ESS" />
   <ElementType name="channel2Dot4" 	dt:type="ui4" />   
   <ElementType name="channel5Dot0" 	dt:type="ui4" />
   <ElementType name="deviceMode" 		dt:type="enumeration" dt:values="infrastructure bridge repeater station" />

   <ElementType name="primaryProfile" content="eltOnly">
      <element type="authentication" 	minOccurs="1" maxOccurs="1" />
      <element type="encryption" 		minOccurs="1" maxOccurs="1" />
      <element type="networkKey" 		minOccurs="1" maxOccurs="1" />
      <element type="keyIndex" 			minOccurs="0" maxOccurs="1" />
      <element type="keyProvidedAutomatically" minOccurs="1" maxOccurs="1" />
      <element type="ieee802Dot1xEnabled" minOccurs="1" maxOccurs="1" />
      <element type="eapMethod" 		minOccurs="0" maxOccurs="1" />
      <element type="timeToLive" 		minOccurs="0" maxOccurs="1" />
   </ElementType>

   <ElementType name="optionalProfile" content="eltOnly"> 
      <element type="authentication" 	minOccurs="1" maxOccurs="1" />
      <element type="encryption" 		minOccurs="1" maxOccurs="1" />
      <element type="networkKey" 		minOccurs="1" maxOccurs="1" />
      <element type="keyIndex" 			minOccurs="0" maxOccurs="1" />
      <element type="keyProvidedAutomatically" minOccurs="1" maxOccurs="1" />
      <element type="ieee802Dot1xEnabled" 	minOccurs="1" maxOccurs="1" />
      <element type="eapMethod" 		minOccurs="0" maxOccurs="1" />
      <element type="timeToLive" 		minOccurs="0" maxOccurs="1" />
   </ElementType>

   <ElementType name="authentication" 	dt:type="enumeration" dt:values="open shared WPA-NONE WPA WPAPSK WPA2 WPA2PSK"/>
   <ElementType name="encryption" 		dt:type="enumeration" dt:values="none WEP TKIP AES"/>
   <ElementType name="networkKey" 		dt:type="string" content="textOnly" dt:minLength="5" dt:maxLength="64" />
   <ElementType name="keyIndex" 		dt:type="ui4" />
   <ElementType name="keyProvidedAutomatically" dt:type="enumeration" dt:values="0 1 true false TRUE FALSE True False" />	
   <ElementType name="ieee802Dot1xEnabled" 	dt:type="enumeration" dt:values="0 1 true false TRUE FALSE True False" />	
   <ElementType name="eapMethod" 		dt:type="enumeration" dt:values="EAP-TLS PEAP-EAP-MSCHAPv2 PEAP-EAP-TLS"/>
   <ElementType name="timeToLive" 		dt:type="ui4" />

   <ElementType name="config" content="eltOnly">
      <element type="configId" 			minOccurs="1" maxOccurs="1" />
      <element type="configHash" 		minOccurs="0" maxOccurs="1" />
      <element type="configAuthorId" 	minOccurs="1" maxOccurs="1" />
      <element type="configAuthor" 		minOccurs="1" maxOccurs="1" />
   </ElementType>   

   <ElementType name="configId" 		dt:type="string" content="textOnly" dt:minLength="36" dt:maxLength="36" />
   <ElementType name="configHash" 		dt:type="string" content="textOnly" dt:minLength="20" dt:maxLength="20" />
   <ElementType name="configAuthorId" 	dt:type="string" content="textOnly" dt:minLength="36" dt:maxLength="36" />
   <ElementType name="configAuthor" 	dt:type="string" content="textOnly" dt:minLength="1"  dt:maxLength="128" />

</Schema>
